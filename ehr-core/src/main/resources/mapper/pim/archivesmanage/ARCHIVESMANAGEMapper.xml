<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper	namespace="cn.ibizlab.ehr.core.pim.mapper.ARCHIVESMANAGEMapper">

    <!--该方法用于重写mybatis中selectById方法，以实现查询逻辑属性-->
	<select id="selectById"  resultMap="ARCHIVESMANAGEResultMap" databaseId="mysql">
        <![CDATA[select * from v_archivesmanage t1     where  archivesmanageid=#{id}]]>
    </select>
	<select id="selectById"  resultMap="ARCHIVESMANAGEResultMap" databaseId="oracle">
        <![CDATA[select * from v_archivesmanage t1     where  archivesmanageid=#{id}]]>
    </select>

    <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="ARCHIVESMANAGEResultMap" type="cn.ibizlab.ehr.core.pim.domain.ARCHIVESMANAGE" autoMapping="true">
        <id property="archivesmanageid" column="archivesmanageid" /><!--主键字段映射-->
            
		
    </resultMap>


    <!--数据集合[Default]-->
	 <select id="searchDefault"  parameterType="cn.ibizlab.ehr.core.pim.filter.ARCHIVESMANAGESearchContext"  resultMap="ARCHIVESMANAGEResultMap">
         select t1.* from (
                <include refid="Default" />
         )t1
         <where><if test="ew!=null and ew.sqlSegment!=null and !ew.emptyOfWhere">${ew.sqlSegment}</if></where>
         <if test="ew!=null and ew.sqlSegment!=null and ew.emptyOfWhere">${ew.sqlSegment}</if>
     </select>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="mysql">
       <![CDATA[	SELECT t1.`ARCHIVESMANAGEID`, t1.`ARCHIVESMANAGENAME`, t1.`ARCHIVESNO`, t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`ENABLE`, t1.`UPDATEDATE`, t1.`UPDATEMAN` FROM `T_ARCHIVESMANAGE` t1  
				 WHERE 	t1.ENABLE = 1
			]]>
    </sql>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="oracle">
       <![CDATA[	SELECT t1.ARCHIVESMANAGEID, t1.ARCHIVESMANAGENAME, t1.ARCHIVESNO, t1.CREATEDATE, t1.CREATEMAN, t1.ENABLE, t1.UPDATEDATE, t1.UPDATEMAN FROM T_ARCHIVESMANAGE t1  
				 WHERE 	t1.ENABLE = 1
			]]>
    </sql>

</mapper>

