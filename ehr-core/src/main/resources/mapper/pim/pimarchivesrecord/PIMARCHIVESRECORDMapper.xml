    <resultMap id="PIMARCHIVESRECORDResultMap" type="cn.ibizlab.ehr.core.pim.domain.PIMARCHIVESRECORD" autoMapping="true">
        <id property="pimarchivesrecordid" column="pimarchivesrecordid" /><!--主键字段映射-->
