<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper	namespace="cn.ibizlab.ehr.core.soc.mapper.SOCCOMPANYWELMapper">

    <!--该方法用于重写mybatis中selectById方法，以实现查询逻辑属性-->

    <!--通过mybatis将查询结果注入到entity中,通过配置autoMapping="true"由mybatis自动处理映射关系 -->
    <resultMap id="SOCCOMPANYWELResultMap" type="cn.ibizlab.ehr.core.soc.domain.SOCCOMPANYWEL" autoMapping="true">
        <id property="soccompanywelid" column="soccompanywelid" /><!--主键字段映射-->
		<result property="ormorgid" column="ormorgid" />
		<result property="socselfarebaseid" column="socselfarebaseid" />
            
		
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="ormorg" javaType="cn.ibizlab.ehr.core.orm.domain.ORMORG" column="ormorgid" select="cn.ibizlab.ehr.core.orm.mapper.ORMORGMapper.selectById" fetchType="lazy"></association>
		<!--通过mybatis自动注入关系属性[主实体]，fetchType="lazy"为懒加载配置 -->
		<association property="socselfarebase" javaType="cn.ibizlab.ehr.core.soc.domain.SOCSELFAREBASE" column="socselfarebaseid" select="cn.ibizlab.ehr.core.soc.mapper.SOCSELFAREBASEMapper.selectById" fetchType="lazy"></association>
    </resultMap>

	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_SOCCOMPANYWEL_ORMORG_ORMORGID] -->
	 <select id="selectByOrmorgid" resultMap="SOCCOMPANYWELResultMap">
         select t1.* from (
                <include refid="Default" />
         ) t1
         where  ormorgid=#{orgid}
     </select>
	<!--关系实体暴露select方法供主实体通过外键查询关系实体数据[实体关系名称:DER1N_SOCCOMPANYWEL_SOCSELFAREBASE_SOCSELFAREBASEID] -->
	 <select id="selectBySocselfarebaseid" resultMap="SOCCOMPANYWELResultMap">
         select t1.* from (
                <include refid="Default" />
         ) t1
         where  socselfarebaseid=#{socselfarebaseid}
     </select>

    <!--数据集合[Default]-->
	 <select id="searchDefault"  parameterType="cn.ibizlab.ehr.core.soc.filter.SOCCOMPANYWELSearchContext"  resultMap="SOCCOMPANYWELResultMap">
         select t1.* from (
                <include refid="Default" />
         )t1
         <where><if test="ew!=null and ew.sqlSegment!=null and !ew.emptyOfWhere">${ew.sqlSegment}</if></where>
         <if test="ew!=null and ew.sqlSegment!=null and ew.emptyOfWhere">${ew.sqlSegment}</if>
     </select>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="mysql">
       <![CDATA[	SELECT t1.`CREATEDATE`, t1.`CREATEMAN`, t1.`ENABLE`, t1.`ORMORGID` AS `ORGID`, t1.`ORMORGID`, t11.`ORGNAME` AS `ORMORGNAME`, t1.`SOCCOMPANYWELID`, t1.`SOCCOMPANYWELNAME`, t1.`SOCSELFAREBASEID`, t21.`SOCSELFAREBASENAME`, t1.`UPDATEDATE`, t1.`UPDATEMAN` FROM `T_SOCCOMPANYWEL` t1  LEFT JOIN T_ORG t11 ON t1.ORMORGID = t11.ORGID  LEFT JOIN T_SOCSELFAREBASE t21 ON t1.SOCSELFAREBASEID = t21.SOCSELFAREBASEID  
				 WHERE 	t1.ENABLE = 1
			]]>
    </sql>

    <!--数据查询[Default]-->
    <sql id="Default" databaseId="oracle">
       <![CDATA[	SELECT t1.CREATEDATE, t1.CREATEMAN, t1.ENABLE, t1.ORMORGID AS ORGID, t1.ORMORGID, t11.ORGNAME AS ORMORGNAME, t1.SOCCOMPANYWELID, t1.SOCCOMPANYWELNAME, t1.SOCSELFAREBASEID, t21.SOCSELFAREBASENAME, t1.UPDATEDATE, t1.UPDATEMAN FROM T_SOCCOMPANYWEL t1  LEFT JOIN T_SRFORG t11 ON t1.ORMORGID = t11.ORGID  LEFT JOIN T_SOCSELFAREBASE t21 ON t1.SOCSELFAREBASEID = t21.SOCSELFAREBASEID  
				 WHERE 	t1.ENABLE = 1
			]]>
    </sql>

