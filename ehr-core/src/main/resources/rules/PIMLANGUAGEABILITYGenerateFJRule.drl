global cn.ibizlab.ehr.core.pim.domain.PIMLANGUAGEABILITY pimlanguageabilitygeneratefjdefault;
global cn.ibizlab.ehr.core.pim.domain.PIMENCLOSURE pimlanguageabilitygeneratefjenclosure;
global cn.ibizlab.ehr.core.pim.service.IPIMENCLOSUREService pimenclosureservice;
